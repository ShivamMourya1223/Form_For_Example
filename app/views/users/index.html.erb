<h2>index</h2>

<h1>hello</h1>
<a href ="/users/new">New</a>
<table border=1>
<tr>
	<th>Name</th>
	<th>Age</th>
	<th>City</th>
	<th>Edit</th>
	<th>Delete</th>

</tr>
<% @users.each do |var| %>
  <tr>
	  <td><%=var.name%></td>
	  <td><%=var.age%></td>
	  <td><%=var.city%></td>
	  <td><%= link_to 'edit', edit_user_path(var.id) %></td>
	   <td><%= link_to 'delete',user_path(var),method: :delete,
	    data: { confirm:"are you sure" } %></td>
  </tr>
  <%end%>
</table>
